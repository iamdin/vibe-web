{
	"name": "vibe-web",
	"type": "module",
	"bin": {
		"vibe-web": "./dist/server.js"
	},
	"scripts": {
		"dev": "bun --watch src/server.ts",
		"build": "bun build src/server.ts --target bun --outdir ./dist",
		"start": "NODE_ENV=production bun dist/server.js",
		"typecheck": "tsc --noEmit",
		"clean": "git clean -xdf node_modules .turbo dist"
	},
	"dependencies": {
		"@anthropic-ai/claude-agent-sdk": "^0.1.12",
		"@elysiajs/cors": "^1.4.0",
		"@elysiajs/node": "^1.4.1",
		"@elysiajs/static": "^1.4.0",
		"@orpc/openapi": "^1.9.3",
		"@orpc/server": "^1.8.6",
		"ai": "^5.0.63",
		"elysia": "^1.4.11",
		"uuid": "^13.0.0",
		"ws": "^8.18.3",
		"zod": "^3.24.1"
	},
	"devDependencies": {
		"@types/bun": "^1.3.0",
		"@vibe-web/agents": "workspace:*",
		"@vibe-web/server-rpc": "workspace:*",
		"@vibe-web/typescript": "workspace:*",
		"tsdown": "^0.13.5",
		"tsx": "^4.20.5"
	}
}
