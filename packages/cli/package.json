{
	"name": "vibe-web",
	"type": "module",
	"bin": {
		"vibe-web": "./dist/server.js"
	},
	"scripts": {
		"dev": "tsx watch src/server.ts",
		"build": "tsdown --entry src/server.ts",
		"start": "node dist/server.js",
		"typecheck": "tsc --noEmit",
		"clean": "git clean -xdf node_modules .turbo dist"
	},
	"dependencies": {
		"@anthropic-ai/claude-code": "^1.0.127",
		"@orpc/server": "^1.8.6",
		"ai": "^5.0.63",
		"cors": "^2.8.5",
		"express": "^5.1.0",
		"uuid": "^13.0.0",
		"ws": "^8.18.3",
		"zod": "^4.1.8"
	},
	"devDependencies": {
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.3",
		"@types/ws": "^8.18.1",
		"@vibe-web/agents": "workspace:*",
		"@vibe-web/server-trpc": "workspace:*",
		"@vibe-web/typescript": "workspace:*",
		"tsx": "^4.20.5",
		"tsdown": "^0.13.5"
	}
}
