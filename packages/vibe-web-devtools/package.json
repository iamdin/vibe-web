{
	"name": "vibe-web-devtools",
	"version": "0.0.3",
	"type": "module",
	"description": "A vite plugin that allow you direct chat with your claude code",
	"keywords": [
		"unplugin",
		"vite",
		"webpack",
		"rspack"
	],
	"files": [
		"dist"
	],
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"./vite": "./dist/vite.js"
	},
	"typesVersions": {
		"*": {
			"*": [
				"./dist/*",
				"./*"
			]
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"stub": "tsdown",
		"build": "tsdown",
		"typecheck": "tsc --noEmit",
		"clean": "git clean -xdf node_modules dist .turbo"
	},
	"peerDependencies": {
		"vite": ">=3"
	},
	"peerDependenciesMeta": {
		"vite": {
			"optional": true
		}
	},
	"dependencies": {
		"@anthropic-ai/claude-code": "^1.0.127",
		"@babel/core": "^7.28.3",
		"@babel/generator": "^7.28.3",
		"@babel/parser": "^7.28.3",
		"@babel/traverse": "^7.28.3",
		"@babel/types": "^7.28.2",
		"@orpc/server": "^1.8.6",
		"@types/ws": "^8.18.1",
		"@vue/babel-plugin-jsx": "^1.5.0",
		"@vue/compiler-dom": "^3.5.19",
		"@vue/compiler-sfc": "^3.5.19",
		"ai": "^5.0.63",
		"magic-string": "^0.30.18",
		"pathe": "^2.0.3",
		"sirv": "^3.0.2",
		"tiny-invariant": "^1.3.3",
		"ws": "^8.18.3",
		"zod": "^4.1.8"
	},
	"devDependencies": {
		"@types/babel__core": "^7.20.5",
		"@types/babel__generator": "^7.27.0",
		"@types/babel__traverse": "^7.28.0",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.3",
		"@types/node": "^22.16.5",
		"@vibe-web/agents": "workspace:*",
		"@vibe-web/code-inspector-node": "workspace:*",
		"@vibe-web/server-trpc": "workspace:*",
		"publint": "^0.3.12",
		"tsdown": "^0.13.5",
		"vitest": "^3.2.4"
	}
}
